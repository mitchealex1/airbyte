{
  "documentationUrl": "",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "New Relic Spec",
    "type": "object",
    "required": ["api_url", "user_key"],
    "additionalProperties": false,
    "properties": {
      "api_url": {
        "type": "string",
        "description": "The URL for the New Relic NerdGraph API Endpoint. This will differ based on your New Relic region.",
        "examples": [
          "https://api.newrelic.com/graphql",
          "https://api.eu.newrelic.com/graphql"
        ],
        "default": "https://api.newrelic.com/graphql"
      },
      "user_key": {
        "type": "string",
        "description": "New Relic User Key."
      }
    }
  }
}
